% FritzJohnHomotopy.input
CONFIG
 UserHomotopy:2;
END;
INPUT
 function F,G1,G2,patch;
 variable_group x1,x2;
 variable_group l0,l1;
 constant y1,y2,eps,a0,a1;
 pathvariable T;
 parameter t;

 t = T;
 y1 = 0.5;
 y2 = 0.8;
 eps = 1.1 + 0.7*I;
 a0 = -1.3;
 a1 = 1.7;

 F = x2^2 + x1^2*(x1-1)^3*(x1-2) - t*eps;
 G1 = l0*(x1-y1) + l1*x1*(x1-1)^2*(6*x1^2-13*x1+4);
 G2 = l0*(x2-y2) + 2*l1*x2;
 patch = a0*l0 + a1*l1 - 1;
END;
